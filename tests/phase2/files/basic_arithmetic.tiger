/* Test: basic integer arithmetic operators */

program basic_arithmetic
let
begin

    function main() : int
    begin
        let
            var a, b : int := 5;
            var c : int;
        begin
            c := 200 + 100;
            printi(c);
            
            c := a + 50;
            printi(c);
                
            c := a + b;
            printi(c);

            c := 200 - 100;
            printi(c);
            
            c := a - 50;
            printi(c);

            c := a - b;
            printi(c);

            c := 200 * 100;
            printi(c);

            c := a * 50;
            printi(c);

            c := a * b;
            printi(c);
            
            c := 200 / 100;
            printi(c);
            
            c := a / 1;
            printi(c);

            c := a / b;
            printi(c);	
        end
    return 0;
    end

end
