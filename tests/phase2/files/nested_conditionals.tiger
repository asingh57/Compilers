/* Test: nested conditionals */

program nested_conditionals
let
begin

    function main() : int 
    begin
        let
            var a : int := 1;
            var b : int := 2;
            var c : int := 3;
        begin
            if a > b then
                if b > c then
                    printi(1);
                else
                    printi(2);
                endif;
            else
                if b < c then
                    printi(3);
                else
                    printi(4);
                endif;
            endif;
        end
        return 0;
    end
    
end
