/* Test: basic array operations */

program basic_array
let
begin

    function main() : int
    begin
        let
            type intArray = array[3] of int;
            var a : intArray;
        begin
            a[0] := 10;
            a[1] := 20;
            a[2] := a[3] + a[1];

            printi(a[2]);
        end
        return 0;
    end

end
